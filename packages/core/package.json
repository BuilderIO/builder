{
  "name": "@builder.io/sdk",
  "version": "2.2.2",
  "unpkg": "./dist/index.browser.js",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderIO/builder"
  },
  "license": "MIT",
  "scripts": {
    "_unlink": "yarn unlink *",
    "_install": "yarn install",
    "docs": "yarn run docs:generate && yarn run docs:cleanup",
    "docs:generate": "typedoc",
    "docs:cleanup": "node scripts/cleanup-generated-docs",
    "build": "yarn run tsc && rollup -c",
    "watch": "rollup -cw",
    "start": "yarn run watch",
    "tsc": "tsc --module commonjs",
    "release": "yarn run build && yarn npm publish",
    "release:patch": "yarn run build && yarn version patch && yarn npm publish",
    "release:minor": "yarn run build && yarn version minor && yarn npm publish",
    "release:patch:nightly": "yarn run build && yarn version patch && yarn npm publish",
    "release:dev": "yarn run build && yarn version prerelease && yarn npm publish --tag dev",
    "test": "jest --color"
  },
  "devDependencies": {
    "@medv/finder": "^1.1.1",
    "@types/axios": "^0.14.0",
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "27.4.0",
    "@types/js-cookie": "^2.1.0",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/node": "^20.4.5",
    "@types/node-fetch": "^1.6.7",
    "@types/qs": "^6.5.1",
    "@types/query-string": "^5.0.1",
    "@types/ua-parser-js": "^0.7.32",
    "@types/virtual-dom": "^2.0.27",
    "esbuild": "0.14.14",
    "esbuild-runner": "2.2.1",
    "glob": "^7.2.3",
    "glob-promise": "^4.2.2",
    "jest": "27.4.7",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "rollup": "^2.38.5",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.23.0",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "hash-sum": "^2.0.0",
    "node-fetch": "^2.3.0",
    "tslib": "^1.10.0"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "gitHead": "4d96fbc32864698afbb355ab991c6d90be991951"
}
