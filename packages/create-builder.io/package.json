{
  "name": "create-builder.io",
  "version": "0.0.1",
  "description": "Quickly create a new builder.io application: npm init builder.io",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build.bundle": "rollup -c",
    "minify": "terser --compress --mangle --toplevel --output dist/index.js -- dist/index.js",
    "build": "npm run build.bundle && npm run minify",
    "build.dev": "npm run build.bundle",
    "dev": "npm run build.dev && npm start",
    "version": "npm build",
    "fmt": "prettier --write src",
    "release": "np"
  },
  "files": [
    "dist/index.js"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "bin": {
    "create-builder.io": "dist/index.js",
    "create-builderio": "dist/index.js",
    "create-builder": "dist/index.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/cli-progress": "^3.9.2",
    "@types/cli-spinner": "0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "16.11.6",
    "@types/yauzl": "2.9.2",
    "cli-spinner": "^0.2.10",
    "colorette": "2.0.16",
    "https-proxy-agent": "^5.0.0",
    "np": "7.5.0",
    "prettier": "^2.4.1",
    "rollup": "2.59.0",
    "sisteransi": "^1.0.5",
    "terser": "5.9.0",
    "typescript": "4.4.4",
    "yauzl": "^2.10.0",
    "commander": "^8.3.0",
    "fs-extra": "^10.0.0",
    "open": "^8.4.0"
  },
  "author": "Builder.io Team",
  "license": "MIT",
  "keywords": [
    "builder",
    "builder.io",
    "drag-and-drop"
  ],
  "dependencies": {
    "replace-in-file": "^6.3.2"
  }
}
