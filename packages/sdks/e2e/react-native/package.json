    "@babel/core": "^7.20.0",
    "@config-plugins/detox": "^8.0.0",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.0.7",
    "compression": "^1.7.4",
