{
  "name": "@e2e/sveltekit",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "NODE_ENV=browser svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "serve": "vite preview",
    "test": "SERVER_NAME=sveltekit yarn nx test:e2e @sdk/tests"
  },
  "dependencies": {
    "@builder.io/sdk-svelte": "workspace:*",
    "@sdk/tests": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.7",
    "nx": "^19.6.5",
    "nx-cloud": "^19.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tslib": "^2.4.1",
    "typescript": "^5.1.6",
    "vite": "^4.0.0"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.svelte-kit"
        ]
      }
    }
  }
}
