{
  "name": "@e2e/sveltekit",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "NODE_ENV=browser svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "serve": "vite preview"
  },
  "dependencies": {
    "@builder.io/sdk-svelte": "workspace:*",
    "@e2e/tests": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.16.2",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.6",
    "tslib": "^2.4.1",
    "typescript": "^5.1.6",
    "vite": "^4.0.0"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.svelte-kit"
        ]
      }
    }
  }
