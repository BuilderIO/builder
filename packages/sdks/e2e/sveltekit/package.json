{
  "name": "@e2e/sveltekit",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "NODE_ENV=browser svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "serve": "vite preview",
    "test": "SERVER_NAME=sveltekit yarn g:nx test:e2e @sdk/tests"
  },
  "dependencies": {
    "@builder.io/sdk-svelte": "workspace:*",
    "@sdk/tests": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.7.1",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.5",
    "tslib": "^2.4.1",
    "typescript": "^5.1.6",
    "vite": "^5.4.9"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.svelte-kit"
        ]
      }
    }
  }
}
