    "eslint-plugin-react-hooks": "^5.1.0",
    "express": "^4.21.2",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.3",
    "morgan": "^1.10.0",
    "react-test-renderer": "18.3.1",
    "serve": "^14.2.4",
    "typescript": "^5.3.3"
