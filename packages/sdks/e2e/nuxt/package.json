{
  "name": "@e2e/nuxt",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "serve": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "SERVER_NAME=nuxt yarn nx test:e2e @sdk/tests"
  },
  "dependencies": {
    "@builder.io/sdk-vue": "workspace:*",
    "@sdk/tests": "workspace:*",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^18",
    "nuxt": "^3.8.1",
    "nx": "16.10.0",
    "nx-cloud": "16.5.2"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.nuxt",
          "{projectRoot}/.output"
        ]
      }
    }
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
