{
  "name": "@e2e/qwik-city",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "test": "SERVER_NAME=qwik-city yarn g:nx test:e2e @sdk/tests",
    "serve": "yarn run preview"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.17.1",
    "@builder.io/qwik-city": "^1.17.1",
    "@types/node": "^18.11.18",
    "node-fetch": "3.3.0",
    "typescript": "^5.1.6",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@builder.io/sdk-qwik": "workspace:*",
    "@sdk/tests": "workspace:*"
  }
}
