{
  "name": "@builder.io/e2e-vue2",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "rename-hack": "mv node_modules/@builder.io/sdk-vue/vue2/package.json node_modules/@builder.io/sdk-vue/vue2/--package.json",
    "rename-hack:undo": "mv node_modules/@builder.io/sdk-vue/vue2/--package.json node_modules/@builder.io/sdk-vue/vue2/package.json",
    "build": "tsc --noEmit && yarn rename-hack && vite build && yarn rename-hack:undo",
    "dev": "tsc --noEmit && vite",
    "e2e": "../../node_modules/.bin/playwright test",
    "serve": "vite preview"
  },
  "dependencies": {
    "@builder.io/sdk-vue": "workspace:*",
    "@builder.io/sdks-e2e-tests": "workspace:*",
    "vue": "^2.7",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^1.1.2",
    "typescript": "^4",
    "vite": "^2.9.13",
    "vite-plugin-components": "^0.13.2",
    "vue-template-compiler": "^2.7.8"
  }
}
