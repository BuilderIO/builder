{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Node16",
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "declaration": true,
    "outDir": "./dist",
    "declarationDir": "./types",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "rootDir": "./"
  },
  "include": ["types.d.ts", "types-hack.d.ts", "src"],
  "exclude": ["src/blocks/form/form.tsx"]
}
