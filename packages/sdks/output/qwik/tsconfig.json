{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "./types",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "rootDir": "./",
    "paths": {
      "placeholder-runtime": ["./src/functions/evaluate/placeholder-runtime.js"]
    }
  },
  "include": ["types.d.ts", "types-hack.d.ts", "src"],
  "exclude": ["src/blocks/form/form.tsx"]
}
