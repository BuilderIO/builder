{
  "name": "@builder.io/sdk-qwik",
  "version": "0.5.5",
  "description": "Builder.io Qwik SDK",
  "type": "module",
  "main": "./lib/index.qwik.cjs",
  "module": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "README.md",
    "package.json",
    "types-hacks.d.ts",
    "types",
    "lib"
  ],
  "imports": {
    "#code-evaluator": {
      "edge-routine": "./src/functions/evaluate/non-node-runtime/index.ts",
      "workerd": "./src/functions/evaluate/non-node-runtime/index.ts",
      "deno": "./src/functions/evaluate/non-node-runtime/index.ts",
      "lagon": "./src/functions/evaluate/non-node-runtime/index.ts",
      "netlify": "./src/functions/evaluate/non-node-runtime/index.ts",
      "edge-light": "./src/functions/evaluate/non-node-runtime/index.ts",
      "bun": "./src/functions/evaluate/non-node-runtime/index.ts",
      "browser": "./src/functions/evaluate/browser-runtime/index.ts",
      "default": "./src/functions/evaluate/browser-runtime/index.ts",
      "react-native": "./src/functions/evaluate/node-runtime/index.ts",
      "electron": "./src/functions/evaluate/node-runtime/index.ts",
      "node": "./src/functions/evaluate/node-runtime/index.ts"
    }
  },
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc && vite build --mode lib"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.6",
    "@types/node": "latest",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.2"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">=1.0.0",
    "isolated-vm": "^4.6.0",
    "undici": "^5.14.0"
  },
  "nx": {
    "implicitDependencies": [
      "@builder.io/sdks"
    ],
    "targets": {
      "build": {
        "dependsOn": [
          "^build:qwik",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/lib",
          "{projectRoot}/types",
          "{projectRoot}/dist"
        ]
      }
    }
  }
}
