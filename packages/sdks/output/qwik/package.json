{
  "name": "@builder.io/sdk-qwik",
  "version": "0.7.1-4",
  "description": "Builder.io Qwik SDK",
  "type": "module",
  "main": "./lib/index.qwik.js",
  "module": "./lib/index.qwik.js",
  "qwik": "./lib/index.qwik.js",
  "types": "./types/src/index.d.ts",
  "files": [
    "README.md",
    "package.json",
    "types-hacks.d.ts",
    "types",
    "lib"
  ],
  "exports": {
    ".": {
      "node": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "browser": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "edge-routine": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "workerd": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "deno": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "lagon": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "netlify": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "edge-light": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "bun": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "electron": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      },
      "default": {
        "types": "./types/src/index.d.ts",
        "import": "./lib/index.qwik.js"
      }
    },
    "./edge": {
      "types": "./types/src/index.d.ts",
      "import": "./lib/index.qwik.js"
    },
    "./node": {
      "types": "./types/src/index.d.ts",
      "import": "./lib/index.qwik.js"
    },
    "./browser": {
      "types": "./types/src/index.d.ts",
      "import": "./lib/index.qwik.js"
    }
  },
  "scripts": {
    "release": "npm publish",
    "typecheck": "tsc --noEmit",
    "tsc": "tsc",
    "build": "nx run-many -p @builder.io/sdk-qwik -t tsc build:one",
    "build:one": "vite build --mode lib"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.6",
    "@builder.io/sdks": "workspace:*",
    "@types/node": "latest",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.2",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "isolated-vm": "^4.6.0"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">=1.0.0",
    "undici": "^5.14.0"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build:qwik",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/lib",
          "{projectRoot}/types"
        ]
      }
    }
  }
}
