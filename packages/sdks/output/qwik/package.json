  },
  "devDependencies": {
    "@builder.io/qwik": "^1.0.0",
    "@types/node": "latest",
    "typescript": "^5.1.6",
    "vite": "^3.0.4"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">=1.0.0"
  }
}
