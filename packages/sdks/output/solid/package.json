{
  "name": "@builder.io/sdk-solid",
  "version": "0.5.8",
  "description": "",
  "type": "module",
  "main": "./lib/browser/index.jsx",
  "module": "./lib/browser/index.jsx",
  "exports": {
    ".": {
      "default": "./lib/browser/index.jsx",
      "solid": "./lib/browser/index.jsx"
    }
  },
  "scripts": {
    "build:all": "nx run-many -p @builder.io/sdk-qwik -t build:node build:edge build:browser",
    "build:node": "SDK_ENV=node vite build --mode lib",
    "build:edge": "SDK_ENV=edge vite build --mode lib",
    "build": "SDK_ENV=browser vite build --mode lib"
  },
  "dependencies": {
    "solid-styled-components": "^0.27.6"
  },
  "peerDependencies": {
    "solid-js": ">=1.5.7"
  },
  "devDependencies": {
    "@builder.io/sdks": "workspace:*",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "solid-js": "^1.7.11",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build:solid",
          "^build"
        ]
      }
    }
  }
