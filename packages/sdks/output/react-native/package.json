{
  "name": "@builder.io/sdk-react-native",
  "description": "Builder.io SDK for React Native",
  "version": "0.5.8",
  "type": "module",
  "main": "./lib/browser/index.cjs",
  "module": "./lib/browser/index.cjs",
  "exports": {
    ".": "./lib/browser/index.cjs"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "nx run-many -p @builder.io/sdk-react-native -t build:node build:edge build:browser",
    "build:node": "SDK_ENV=node vite build --mode lib",
    "build:edge": "SDK_ENV=edge vite build --mode lib",
    "build:browser": "SDK_ENV=browser vite build --mode lib"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "react-native-render-html": "^6.3.4",
    "react-native-storage": "^1.0.1",
    "react-native-video": "^5.1.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-native": ">=0.64.3",
    "react-native-url-polyfill": "^1.3.0"
  },
  "devDependencies": {
    "@builder.io/sdks": "workspace:*",
    "@tsconfig/react-native": "^2.0.3",
    "@types/react-native-video": "^5.0.9",
    "@vitejs/plugin-react": "^4.0.4",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "react-native-url-polyfill": "^1.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "vite": "^4.4.9"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build:react-native",
          "^build"
        ]
      }
    }
  }
}
