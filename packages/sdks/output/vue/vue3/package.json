{
  "name": "@builder.io/sdk-vue-3",
  "type": "module",
  "private": true,
  "main": "./lib/node/index.cjs",
  "module": "./lib/node/index.js",
  "files": [
    "lib"
  ],
  "sideEffects": [
    "./lib/browser/style.css",
    "./lib/node/style.css",
    "./lib/edge/style.css"
  ],
  "scripts": {
    "build-inline-fns": "yarn g:build-inline-fns",
    "build": "nx run-many -p @builder.io/sdk-vue-3 -t build:node build:edge build:browser",
    "vite-build": "vite build --mode lib",
    "build:node": "SDK_ENV=node yarn vite-build",
    "build:edge": "SDK_ENV=edge yarn vite-build",
    "build:browser": "SDK_ENV=browser yarn vite-build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "isolated-vm": "^4.6.0"
  },
  "devDependencies": {
    "@builder.io/sdks": "workspace:*",
    "@types/node": "^16.11.47",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "typescript": "^5.1.6",
    "vite": "^3.0.4",
    "vite-plugin-dts": "^3.5.2",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.5"
  },
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "nx": {
    "targets": {
      "build-inline-fns": {
        "dependsOn": [
          "^build:vue3"
        ]
      },
      "build": {
        "dependsOn": [
          "build-inline-fns",
          "^build:vue3",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/lib",
          "{projectRoot}/types"
        ]
      }
    }
  }
}
