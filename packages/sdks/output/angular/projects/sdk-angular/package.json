{
  "name": "@builder.io/sdk-angular",
  "version": "0.0.1",
  "files": [
    "*"
  ],
  "scripts": {
    "release": "npm publish",
    "build-inline-fns": "yarn g:build-inline-fns",
    "build": "nx run-many -p @builder.io/sdk-angular -t build:node build:edge build:browser",
    "build:node": "SDK_ENV=node ng build",
    "build:edge": "SDK_ENV=edge ng build",
    "build:browser": "SDK_ENV=browser ng build"
  },
  "peerDependencies": {
    "@angular/common": "^16.2.0",
    "@angular/core": "^16.2.0"
  },
  "dependencies": {
    "isolated-vm": "^4.6.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/common": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@builder.io/sdks": "workspace:*",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "recast": "^0.23.4",
    "rimraf": "^3.0.2",
    "typescript": "~5.1.3"
  },
  "sideEffects": false,
  "nx": {
    "targets": {
      "build-inline-fns": {
        "dependsOn": [
          "^build:angular"
        ]
      },
      "build": {
        "dependsOn": [
          "build-inline-fns",
          "^build:angular",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/lib",
          "{projectRoot}/types"
        ]
      }
    }
  }
}
