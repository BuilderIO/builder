{
  "name": "@builder.io/sdk-angular",
  "version": "0.0.7",
  "files": [
    "lib"
  ],
  "scripts": {
    "release": "npm publish",
    "build-inline-fns": "echo 'not implemented yet'",
    "build": "ng build --project sdk-angular && rm -rf lib/package.json",
    "build:node": "ng build --project sdk-angular --configuration node",
    "build:edge": "ng build --project sdk-angular --configuration edge",
    "build:browser": "ng build --project sdk-angular --configuration browser"
  },
  "dependencies": {
    "isolated-vm": "^5.0.0",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/common": ">=17.3.0",
    "@angular/core": ">=17.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/animations": "^17.3.0",
    "@angular/cli": "^17.3.8",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@builder.io/sdks": "workspace:*",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.11.30",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "nx": "16.10.0",
    "nx-cloud": "16.5.2",
    "recast": "^0.23.4",
    "rimraf": "^3.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.4.2",
    "zone.js": "~0.14.3"
  },
  "sideEffects": false,
  "nx": {
    "targets": {
      "build-inline-fns": {
        "dependsOn": [
          "^build:angular"
        ]
      },
      "build": {
        "dependsOn": [
          "build-inline-fns",
          "^build:angular",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/lib"
        ]
      }
    }
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "esm2022": "./lib/esm2022/builder.io-sdk-angular.mjs",
      "esm": "./lib/esm2022/builder.io-sdk-angular.mjs",
      "default": "./lib/fesm2022/builder.io-sdk-angular.mjs"
    }
  },
  "module": "lib/fesm2022/builder.io-sdk-angular.mjs",
  "typings": "lib/index.d.ts"
}
