{
  "name": "@builder.io/sdk-react-nextjs",
  "description": "Builder.io SDK for React",
  "version": "0.5.8",
  "files": [
    "lib"
  ],
  "types": "./lib/browser/index.d.ts",
  "main": "./lib/node/index.js",
  "module": "./lib/node/index.js",
  "exports": {
    ".": {
      "node": "./lib/node/index.js",
      "browser": "./lib/browser/index.js",
      "edge-routine": "./lib/edge/index.js",
      "workerd": "./lib/edge/index.js",
      "deno": "./lib/edge/index.js",
      "lagon": "./lib/edge/index.js",
      "netlify": "./lib/edge/index.js",
      "edge-light": "./lib/edge/index.js",
      "bun": "./lib/edge/index.js",
      "react-native": "./lib/node/index.js",
      "electron": "./lib/node/index.js",
      "default": "./lib/browser/index.js"
    },
    "./edge": "./lib/edge/index.js",
    "./node": "./lib/node/index.js",
    "./browser": "./lib/browser/index.js"
  },
  "scripts": {
    "prepare": "rimraf lib",
    "build": "nx run-many -p @builder.io/sdk-react-nextjs -t build:node build:edge build:browser",
    "build:browser": "yarn tsc --project tsconfig.json",
    "build:node": "yarn tsc --project tsconfig.node.json",
    "build:edge": "yarn tsc --project tsconfig.edge.json"
  },
  "peerDependencies": {
    "isolated-vm": "^4.6.0",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@builder.io/sdks": "workspace:*",
    "next": "^13.4.4",
    "nx": "^16.6.0",
    "nx-cloud": "^16.2.0",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "lru-cache": "^10.0.0"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build:nextjs",
          "^build"
        ]
      }
    }
  }
}
