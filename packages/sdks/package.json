{
  "name": "@builder.io/sdks",
  "version": "0.0.1",
  "scripts": {
    "build": "mitosis build",
    "lint": "prettier --write .",
    "start": "watch 'npm run build' ./src ./overrides",
    "test": "jest",
    "ci:build": "npm run build",
    "ci:test": "npm run test"
  },
  "exports": {
    "./react/*": "./dist/react/src/*.js",
    "./vue/*": "./dist/vue/src/*.js",
    "./svelte/*": "./dist/svelte/src/*.js",
    "./angular/*": "./dist/angular/src/*.js",
    "./html/*": "./dist/html/src/*.js",
    "./solid/*": "./dist/solid/src/*.js"
  },
  "dependencies": {
    "@builder.io/mitosis-cli": "0.0.8",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@builder.io/mitosis": "0.0.45-5",
    "@jsx-lite/cli": "^1.0.1",
    "@jsx-lite/eslint-plugin": "0.0.2",
    "@swc/core": "^1.2.64",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.13",
    "@types/react-native": "^0.64.12",
    "@vue/compiler-sfc": "^3.1.4",
    "@vue/component-compiler-utils": "^3.2.2",
    "dedent": "^0.7.0",
    "esbuild": "^0.12.17",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^2.6.0",
    "eslint": "^7.21.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "json5": "^2.2.0",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-native": "^0.64.2",
    "react-native-render-html": "^5.1.1",
    "vue": "2.6.12",
    "vue-template-compiler": "2.6.12",
    "watch": "^1.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "esbuild-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ]
  }
}
