{
  "name": "@builder.io/sdks",
  "private": true,
  "scripts": {
    "prepare": "cd ../../ && npx husky install",
    "cz": "cz",
    "build:mitosis": "./node_modules/@builder.io/mitosis-cli/bin/mitosis build",
    "build:outputs": "pnpm --filter \"*/sdk-*\" --filter=!@builder.io/sdk-vue-2 --filter=!@builder.io/sdk-vue-3 run build",
    "build": "pnpm build:mitosis && pnpm build:outputs",
    "start": "watch 'pnpm build:mitosis' ./src ./overrides",
    "test": "vitest",
    "remove-symlinks": "./scripts/remove-resolutions.sh && pnpm",
    "add-symlinks": "./scripts/add-resolutions.sh && pnpm",
    "upgrade-mitosis:dev": "pnpm up @builder.io/mitosis@dev @builder.io/mitosis-cli@dev @builder.io/eslint-plugin-mitosis@dev",
    "upgrade-mitosis": "pnpm up @builder.io/mitosis @builder.io/mitosis-cli @builder.io/eslint-plugin-mitosis",
    "prettier": "prettier .",
    "prettier:check": "prettier --check .",
    "lint": "yarn run prettier --write . && yarn run eslint --fix .",
    "eslint": "eslint . --quiet",
    "eslint:watch": "watch 'pnpm eslint' ./src ./overrides",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "pnpm typecheck --watch",
    "ci:lint": "pnpm prettier:check && pnpm eslint",
    "ci:build": "pnpm typecheck && pnpm build",
    "ci:test": "pnpm test",
    "e2e:install-playwright": "playwright install chromium --with-deps",
    "e2e:build:specs": "pnpm --filter @builder.io/sdks-e2e-tests build",
    "e2e:build:svelte": "pnpm --filter \"@builder.io/e2e-svelte*\" run build",
    "e2e:build:react-native": "pnpm --filter \"@builder.io/e2e-react-native*\" run build",
    "e2e:build:solid": "pnpm --filter \"@builder.io/e2e-solid*\" run build",
    "e2e:build:qwik": "pnpm --filter \"@builder.io/e2e-qwik*\" run build",
    "e2e:build:react": "pnpm --filter \"@builder.io/e2e?(-nextjs)-react\" run build",
    "e2e:build:vue": "pnpm --filter \"@builder.io/e2e-vue*\" run build",
    "e2e:run:svelte": "pnpm --filter \"@builder.io/e2e-svelte*\" run e2e",
    "e2e:run:react-native": "pnpm --filter \"@builder.io/e2e-react-native*\" run e2e",
    "e2e:run:solid": "pnpm --filter \"@builder.io/e2e-solid*\" run e2e",
    "e2e:run:qwik": "pnpm --filter \"@builder.io/e2e-qwik*\" run e2e",
    "e2e:run:react": "pnpm --filter \"@builder.io/e2e?(-nextjs)-react\" run e2e",
    "e2e:run:vue": "pnpm --filter \"@builder.io/e2e-vue*\" run e2e",
    "e2e:build:all": "pnpm --filter=!@builder.io/e2e-rsc run build",
    "e2e:run:all": "pnpm --filter=!@builder.io/e2e-rsc run e2e",
    "release-sdk": "bash ./scripts/release-sdk.sh",
    "release:all": "bash ./scripts/loop-command.sh release-sdk \"svelte react-native solid qwik react vue\"",
    "upgrade-example": "bash ./scripts/upgrade-example.sh",
    "upgrade-example:all": "bash ./scripts/loop-command.sh upgrade-example \"svelte react-native solid qwik react vue\""
  },
  "dependencies": {
    "@builder.io/mitosis": "^0.0.88",
    "@builder.io/mitosis-cli": "^0.0.46",
    "@playwright/test": "^1.27.1",
    "node-fetch": "^2.6.1",
    "seedrandom": "^3.0.5",
    "traverse": "^0.6.6",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.14",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.12.0",
    "eslint-plugin-react": "latest",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "pinst": "^3.0.0",
    "prettier": "^2.5.0",
    "vite": "^4.0.4",
    "vitest": "^0.15.2",
    "watch": "^1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "resolutions": {
    "minimist": "^1.2.7"
  },
  "pnpm": {
    "patchedDependencies": {
      "@sveltejs/package@1.0.0-next.1": "patches/@sveltejs__package@1.0.0-next.1.patch"
    }
  }
}
