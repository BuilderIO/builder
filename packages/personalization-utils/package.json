{
  "name": "@builder.io/personalization-utils",
  "version": "2.0.1-0",
  "description": "Utils for personalization at the edge",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./next": "./dist/next.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "next": [
        "dist/next.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "jest",
    "prettier": "prettier",
    "release:dev": "npm run build && npm version prerelease --no-git-tag-version && npm publish --tag dev",
    "release:major": "npm run build && npm version major && npm publish"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "devDependencies": {
    "@builder.io/sdk": "^1.1.29",
    "jest": "^26.6.3",
    "next": "^13",
    "ts-jest": "^26.4.4",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "json-stringify-deterministic": "^1.0.6"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@builder.io/sdk": "^1.1.20",
    "next": ">=13"
  }
}
