{
  "name": "@builder.io/storybook",
  "version": "0.0.1",
  "description": "embed builder.io in storybook",
  "homepage": "https://github.com/BuilderIO/builder/tree/master/packages/storybook#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderIO/builder.git",
    "directory": "packages/storybook"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --ignore spec.js,test.js --extensions .ts --extensions .js --extensions .tsx && npm run build:types",
    "build:dev": "babel -w src --out-dir dist --ignore spec.js,test.js --extensions .ts --extensions .js --extensions .tsx",
    "build:types": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "test": "test:all",
    "test:all": "jest src",
    "test:dev": "jest src --watch"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/api": "^5.3.14",
    "@storybook/react": "^5.0.3",
    "@types/js-beautify": "^1.8.0",
    "@types/react": "^16.8.8",
    "@types/storybook__react": "^4.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.6",
    "jest": "^24.5.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.15.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addons": "^5.0.1",
    "@storybook/api": "^5.3.14",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
