{
  "name": "@builder.io/mono-repo",
  "description": "This mono-repo includes the new SDKs and their integration tests.",
  "private": true,
  "workspaces": [
    "sdks-tests",
    "@builder.io/sdks",
    "react",
    "core",
    "react-tests/*",
    "sdks/e2e/*",
    "sdks/output/*",
    "sdks/output/vue/packages/*",
    "sdks/output/react/packages/*"
  ],
  "scripts": {
    "e2e-build-gen1-react": "nx run-many -t build -p @e2e/gen1-react @e2e/gen1-next @e2e/gen1-remix",
    "e2e-run-gen1-react": "SDK=oldReact yarn workspace sdks run e2e:run"
  },
  "engines": {
    "yarn": ">= 3.0.0"
  },
  "resolutions": {
    "@sveltejs/package@1.0.0-next.1": "patch:@sveltejs/package@npm:1.0.0-next.1#.yarn/patches/@sveltejs-package-npm-1.0.0-next.1-2fd015530b.patch",
    "minimist": "^1.2.7",
    "json5": "2.2.1",
    "prettier": "2.8.8",
    "@builder.io/react": "workspace:*"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "nx": "^16.5.4",
    "nx-cloud": "^16.1.1",
    "prettier": "^2.8.8"
  }
}
