{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": [
          "build"
        ],
        "accessToken": "NDk4OGFkMjktNDNmZC00MjQzLWEwNjMtZjU3ZDRkN2E0MmI0fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "dev": {
      "dependsOn": [
        "build"
      ]
    },
    "serve": {
      "dependsOn": [
        "build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        "prebuild"
      ],
      "outputs": [
        "{workspaceRoot}/dist"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      }
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [],
    "production": [
      "default"
    ]
  }
}
