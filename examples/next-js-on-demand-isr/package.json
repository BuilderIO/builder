{
  "name": "nextjs-builder-io-landing-page",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@builder.io/react": "^2.0.4",
    "@builder.io/widgets": "^1.2.23",
    "cors": "^2.8.5",
    "next": "^12.2.2",
    "next-seo": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "bunyan": "^1.8.14",
    "bunyan-prettystream": "^0.1.3",
    "next-unused": "^0.0.6",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "webpack": "^5.0.0-beta.30"
  },
  "license": "MIT"
}
