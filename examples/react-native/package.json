{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "link": "yarn link @builder.io/sdk-react-native",
    "unlink": "yarn unlink @builder.io/sdk-react-native",
    "unlink-fr": "npm run unlink && yarn install --force",
    "rm-sdk-src": "rm -rf node_modules/@builder.io/sdk-react-native/src",
    "cp-sdk": "yarn run rm-sdk-src && cp -R ../../packages/sdks/output/react-native/src node_modules/@builder.io/sdk-react-native/src",
    "unlink-and-cp-sdk": "yarn run unlink && yarn run cp-sdk"
  },
  "dependencies": {
    "@builder.io/sdk-react-native": "^0.0.1-40",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "expo": "~44.0.0",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "react-devtools-core": "4.14.0",
